@model Fortis.Website.Models.TestModel
<hr />
<h1>Test Controller</h1>
<ul>
	<li>Item Name: @Model.ContextItemName</li>
	<li>ITestTemplate (Primitive Types):
		<ul>
			<li>
				Standard
				<ul>
					<li>ID: @Model.TestTemplate.ItemId</li>
				</ul>
			</li>
			<li>
				Simple
				<ul>
					<li>Checkbox: @Model.TestTemplate.TestCheckbox</li>
					<li>Date: @Model.TestTemplate.TestDate</li>
					<li>DateTime: @Model.TestTemplate.TestDateTime</li>
					<li>File: @Model.TestTemplate.TestFile</li>
					<li>Image: @Model.TestTemplate.TestImage</li>
					<li>Integer: @Model.TestTemplate.TestInteger</li>
					<li>MultiLine Text: @Model.TestTemplate.TestMultiLineText</li>
					<li>Number: @Model.TestTemplate.TestNumber</li>
					<li>Password: @Model.TestTemplate.TestPassword</li>
					<li>Rich Text: @Model.TestTemplate.TestRichText</li>
					<li>Single-Line Text: @Model.TestTemplate.TestSingleLineText</li>
				</ul>
			</li>
			<li>
				List Types
				<ul>
					<li>
						Checklist:
						<ul>
							@foreach (var id in Model.TestTemplate.TestChecklist)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>Droplist: @Model.TestTemplate.TestDroplist</li>
					<li>Grouped Droplink: @Model.TestTemplate.TestGroupedDroplink</li>
					<li>Grouped Droplist: @Model.TestTemplate.TestGroupedDroplist</li>
					<li>
						Multilist:
						<ul>
							@foreach (var id in Model.TestTemplate.TestMultilist)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						Multilist With Search:
						<ul>
							@foreach (var id in Model.TestTemplate.TestMultilistWithSearch)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						Name Lookup Value List:
						<ul>
							@foreach (var key in Model.TestTemplate.TestNameLookupValueList.AllKeys)
							{
								<li>@key: @Model.TestTemplate.TestNameLookupValueList[key]</li>
							}
						</ul>
					</li>
					<li>
						Name Value List:
						<ul>
							@foreach (var key in Model.TestTemplate.TestNameValueList.AllKeys)
							{
								<li>@key: @Model.TestTemplate.TestNameValueList[key]</li>
							}
						</ul>
					</li>
					<li>
						Treelist:
						<ul>
							@foreach (var id in Model.TestTemplate.TestTreelist)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						TreelistEx:
						<ul>
							@foreach (var id in Model.TestTemplate.TestTreelistEx)
							{
								<li>@id</li>
							}
						</ul>
					</li>
				</ul>
			</li>
			<li>
				Link
				<ul>
					<li>Droplink: @Model.TestTemplate.TestDroplink</li>
					<li>Droptree: @Model.TestTemplate.TestDroptree</li>
					<li>General Link: @Model.TestTemplate.TestGeneralLink</li>
					<li>General Link With Search: @Model.TestTemplate.TestGeneralLinkWithSearch</li>
				</ul>
			</li>
		</ul>
	</li>
	<li>
		ITestTemplateItem (Fortis Types):
		<ul>
			<li>
				Standard
				<ul>
					<li>Children Count: @Model.TestTemplateItem.ItemChildrenCount</li>
					<li>Database: @Model.TestTemplateItem.ItemDatabase</li>
					<li>Display Name: @Model.TestTemplateItem.ItemDisplayName</li>
					<li>Has Children: @Model.TestTemplateItem.ItemHasChildren</li>
					<li>ID: @Model.TestTemplateItem.ItemId</li>
					<li>Is Latest Version: @Model.TestTemplateItem.ItemIsLatestVersion</li>
					<li>Is Standard Values: @Model.TestTemplateItem.ItemIsStandardValues</li>
					<li>Language: @Model.TestTemplateItem.ItemLanguage</li>
					<li>Long ID: @Model.TestTemplateItem.ItemLongId</li>
					<li>Name: @Model.TestTemplateItem.ItemName</li>
					<li>Path: @Model.TestTemplateItem.ItemPath</li>
					<li>Short ID: @Model.TestTemplateItem.ItemShortId</li>
					<li>Template ID: @Model.TestTemplateItem.ItemTemplateId</li>
					<li>Template IDs:
						<ul>
							@foreach (var id in Model.TestTemplateItem.ItemTemplateIds)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>Template Name: @Model.TestTemplateItem.ItemTemplateName</li>
					<li>Version: @Model.TestTemplateItem.ItemVersion</li>
				</ul>
			</li>
			<li>
				Simple
				<ul>
					<li>Checkbox: @Model.TestTemplateItem.TestCheckbox.Value</li>
					<li>Date: @Model.TestTemplateItem.TestDate.Value</li>
					<li>DateTime: @Model.TestTemplateItem.TestDateTime.Value</li>
					<li>File: @Model.TestTemplateItem.TestFile.Value</li>
					<li>Image: @Model.TestTemplateItem.TestImage.Value</li>
					<li>Integer: @Model.TestTemplateItem.TestInteger.Value</li>
					<li>MultiLine Text: @Model.TestTemplateItem.TestMultiLineText.Value</li>
					<li>Number: @Model.TestTemplateItem.TestNumber.Value</li>
					<li>Password: @Model.TestTemplateItem.TestPassword.Value</li>
					<li>Rich Text: @Model.TestTemplateItem.TestRichText.Value</li>
					<li>Single-Line Text: @Model.TestTemplateItem.TestSingleLineText.Value</li>
				</ul>
			</li>
			<li>
				List Types
				<ul>
					<li>
						Checklist:
						<ul>
							@foreach (var id in Model.TestTemplateItem.TestChecklist.Value)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>Droplist: @Model.TestTemplateItem.TestDroplist.Value</li>
					<li>Grouped Droplink: @Model.TestTemplateItem.TestGroupedDroplink.Value</li>
					<li>Grouped Droplist: @Model.TestTemplateItem.TestGroupedDroplist.Value</li>
					<li>
						Multilist:
						<ul>
							@foreach (var id in Model.TestTemplateItem.TestMultilist.Value)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						Multilist With Search:
						<ul>
							@foreach (var id in Model.TestTemplateItem.TestMultilistWithSearch.Value)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						Name Lookup Value List:
						<ul>
							@foreach (var key in Model.TestTemplateItem.TestNameLookupValueList.Value.AllKeys)
							{
								<li>@key: @Model.TestTemplateItem.TestNameLookupValueList.Value[key]</li>
							}
						</ul>
					</li>
					<li>
						Name Value List:
						<ul>
							@foreach (var key in Model.TestTemplateItem.TestNameValueList.Value.AllKeys)
							{
								<li>@key: @Model.TestTemplateItem.TestNameValueList.Value[key]</li>
							}
						</ul>
					</li>
					<li>
						Treelist:
						<ul>
							@foreach (var id in Model.TestTemplateItem.TestTreelist.Value)
							{
								<li>@id</li>
							}
						</ul>
					</li>
					<li>
						TreelistEx:
						<ul>
							@foreach (var id in Model.TestTemplateItem.TestTreelistEx.Value)
							{
								<li>@id</li>
							}
						</ul>
					</li>
				</ul>
			</li>
			<li>
				Link
				<ul>
					<li>Droplink: @Model.TestTemplateItem.TestDroplink.Value</li>
					<li>Droptree: @Model.TestTemplateItem.TestDroptree.Value</li>
					<li>General Link: @Model.TestTemplateItem.TestGeneralLink.Value</li>
					<li>General Link With Search: @Model.TestTemplateItem.TestGeneralLinkWithSearch.Value</li>
				</ul>
			</li>
		</ul>
	</li>
</ul>
